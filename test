    Callback = function()
        forcetrade(game:GetService('Players')[plr])
    end
}

Tab:Button {
    Name = "Force Initiated Trade All ‚öñÔ∏èü§º",
    Description = nil,
    Callback = function()
        for _, plr in pairs(game:GetService('Players'):GetChildren()) do
            if plr.Name ~= localPlayer.Name then
                forcetrade(plr)
            end
            end
    end
}
