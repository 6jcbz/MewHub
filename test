    end
}
Tab:Button {
    Name = "Open Map üó∫Ô∏è",
    Description = nil,
    Callback = function()
        Get = game:GetService("ReplicatedStorage").GET
        function GetMainData()
            for i, v in pairs(debug.getregistry()) do
                if typeof(v) == "function" then
                    for a, b in pairs(debug.getupvalues(v)) do
