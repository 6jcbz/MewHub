Tab:Button {
    Name = "Skip Chat (Slightly Bugged) ‚è≠Ô∏è",
    Description = nil,
    Callback = function()
        Get = game:GetService("ReplicatedStorage").GET
        function GetMainData()
            for i, v in pairs(debug.getregistry()) do
                if typeof(v) == "function" then
                    for a, b in pairs(debug.getupvalues(v)) do
                        if typeof(b) == "table" and rawget(b, "Fishing") then
                            return b
                        end
                    end
                end
            end
        end
        Data = GetMainData()
        AuthKey = debug.getupvalue(Data["Network"]["getAuthKey"], 1)
        local SkipChat = true
        local OldWait
        OldWait =
            hookfunction(
            Data["NPCChat"]["AdvanceSignal"]["wait"],
            function(...)
                if SkipChat then
                    return
                end
                OldWait(...)
            end
        )
        local Old
        Old =
            hookfunction(
            Data["NPCChat"]["say"],
            function(...)
                local Args = {...}
                if SkipChat then
                    for i = 3, #Args do
                        if typeof(Args[i]) == "string" then
                            Args[i] = "."
                        end
                    end
                end
                return Old(unpack(Args))
            end
        )
    end
}
