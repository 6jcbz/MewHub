        local _p = nil
        for _, v in pairs(getgc(true)) do
            if typeof(v) == "table" then
                if rawget(v, "PlayerData") then
                    _p = v
                    break
                end
            end
        end
        print(_p.PlayerData:completeEvent("GivenLeftovers"))
    end
}
